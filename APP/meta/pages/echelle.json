{
  "access": [
    "Authenticated",
    "admin"
  ],
  "components": [
    {
      "components": [
        {
          "name": "heading0",
          "size": "H2",
          "style": {
            "backgroundColor": "rgb(0, 120, 212)",
            "borderRadius": "5px",
            "color": "#e6ecef",
            "fontFamily": "times new roman,times,serif",
            "paddingBottom": "4px",
            "paddingTop": "2px",
            "textAlign": "center"
          },
          "text": "Echelles d'Ã©valuation",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "events": {
                    "click": [
                      {
                        "path": "add-echelle",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ],
                        "type": "popup"
                      }
                    ]
                  },
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Ajouter",
                  "type": "button"
                },
                {
                  "allowFiltering": false,
                  "allowPaging": false,
                  "allowSorting": false,
                  "columns": [
                    {
                      "property": "Id",
                      "title": "Echelle"
                    },
                    {
                      "property": "Val",
                      "title": "Valeur"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": true
                            }
                          ],
                          "buttonStyle": "danger",
                          "events": {
                            "click": [
                              {
                                "condition": "await DialogService.Confirm(\"Voulez-vous vraiment supprimer cette ligne ?\") == true",
                                "error": [
                                  {
                                    "detail": "Unable to delete Echelle",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "DMdel.deleteEchelle",
                                "parameters": [
                                  {
                                    "name": "Id",
                                    "value": "${data.Id}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "await grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          },
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "size": "sm",
                          "text": "",
                          "type": "button"
                        }
                      ],
                      "textAlign": "center",
                      "width": "70px"
                    }
                  ],
                  "data": "${getEchellesResult}",
                  "events": {
                    "loadData": [],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "Id",
                            "value": "${event.Id}"
                          }
                        ],
                        "path": "edit-echelle",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ],
                        "type": "popup"
                      }
                    ]
                  },
                  "filterMode": "advanced",
                  "itemType": "Pes.Models.DMdel.Echelle",
                  "name": "grid0",
                  "title": "",
                  "type": "datagrid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "DMdel.getEchelles",
        "parameters": [],
        "then": [
          {
            "name": "getEchellesResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Pes.Models.DMdel.Echelle>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "Echelle"
}
{
  "access": [
    "Authenticated",
    "admin"
  ],
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "components": [
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "Rubid",
                              "name": "RubidLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Rubrique",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getRubriquesResult}",
                              "name": "Rubid",
                              "placeholder": "Choose Rubrique",
                              "style": {
                                "width": "100%"
                              },
                              "textProperty": "NomRubrique",
                              "type": "dropdown",
                              "value": "${element.Rubid}",
                              "valueProperty": "Id",
                              "valueType": "int?"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "NomElement",
                              "name": "NomElementLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Nom Element",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "maxLength": 300,
                              "name": "NomElement",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "type": "textbox",
                              "value": "${element.NomElement}",
                              "valueType": "string"
                            },
                            {
                              "component": "NomElement",
                              "name": "NomElementRequiredValidator",
                              "style": {
                                "position": "absolute"
                              },
                              "text": "NomElement is required",
                              "type": "required-validator"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              },
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "data": "${element}",
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Element!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "DMdel.createElement",
                        "parameters": [
                          {
                            "name": "Element",
                            "value": "${element}"
                          }
                        ],
                        "then": [
                          {
                            "result": "${element}",
                            "type": "close-popup"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Pes.Models.DMdel.Element",
                  "name": "form0",
                  "type": "template-form",
                  "visible": "${element != null}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "DMdel.getRubriques",
        "parameters": [
          {
            "name": "$filter",
            "value": "r=>r.Sessionid = {Globals.ActiveSession?.Id}"
          }
        ],
        "then": [
          {
            "name": "getRubriquesResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Pes.Models.DMdel.Rubrique>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "element",
        "type": "set",
        "value": "new Pes.Models.DMdel.Element(){Sessionid = Globals.ActiveSession?.Id}",
        "valueType": "Pes.Models.DMdel.Element"
      }
    ]
  },
  "layout": "Main",
  "name": "Add Element"
}
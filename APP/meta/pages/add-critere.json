{
  "access": [
    "Authenticated",
    "admin"
  ],
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "components": [
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "NomCritere",
                              "name": "NomCritereLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Nom Critere",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "maxLength": 300,
                              "name": "NomCritere",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "type": "textbox",
                              "value": "${critere.NomCritere}",
                              "valueType": "string"
                            },
                            {
                              "component": "NomCritere",
                              "name": "NomCritereRequiredValidator",
                              "style": {
                                "position": "absolute"
                              },
                              "text": "NomCritere is required",
                              "type": "required-validator"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              },
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "data": "${critere}",
                  "events": {
                    "submit": [
                      {
                        "code": "critere.Elementid = int.Parse($\"{Elementid}\");",
                        "type": "execute"
                      },
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Critere!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "DMdel.createCritere",
                        "parameters": [
                          {
                            "name": "Critere",
                            "value": "${critere}"
                          }
                        ],
                        "then": [
                          {
                            "result": "${critere}",
                            "type": "close-popup"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Pes.Models.DMdel.Critere",
                  "name": "form0",
                  "type": "template-form",
                  "visible": "${critere != null}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "critere",
        "type": "set",
        "value": "new Pes.Models.DMdel.Critere(){Sessionid = Globals.ActiveSession?.Id}",
        "valueType": "Pes.Models.DMdel.Critere"
      }
    ]
  },
  "layout": "Main",
  "name": "Add Critere"
}